<?xml version="1.0" encoding="utf-8"?>

<!--
	"fadeout.xml" 파일은 FullScreen Mode에서 사용하기 위한 파일임.
-->

<set xmlns:android="http://schemas.android.com/apk/res/android" >

    <alpha
        android:duration="300"
        android:fromAlpha="1"
